%%%%%%%%%% list squeeze

\newcommand{\squeeze}{%
%\setlength {\partopsep}{0in}%
%\setlength {\topsep}{-1.5em}%
\setlength {\itemsep}{-.05in}}
\newcommand{\negskip}{\vspace{-.10in}}

\let\olditemize\itemize
\let\oldenditemize\enditemize
\def\itemize{\olditemize\squeeze\negskip}
\def\enditemize{\oldenditemize\negskip}

\let\olddescription\description
\let\oldenddescription\enddescription
\def\description{\olddescription\squeeze\negskip}
\def\enddescription{\oldenddescription\negskip}

\let\oldenumerate\enumerate
\let\oldendenumerate\endenumerate
\def\enumerate{\oldenumerate\squeeze\negskip}
\def\endenumerate{\oldendenumerate\negskip}

%%%%%%%% display math squeeze

\def\squeezemath{%
\abovedisplayskip 0in%
\belowdisplayskip 0in%
\abovedisplayshortskip 0in%
\belowdisplayshortskip 0in}

\let\oldbegindisplaymath\[
\def\[{\oldbegindisplaymath\squeezemath}

\let\oldequation\equation
\def\equation{\oldequation\squeezemath}

%%%%% headings squeeze

\def\section{\@startsection {section}{1}{\z@}{-1.5ex plus -1ex minus 
 -.2ex}{0.5ex plus .2ex}{\bf}}
\def\subsection{\@startsection{subsection}{2}{\z@}{-1.25ex plus -1ex minus 
 -.2ex}{0.3ex plus .2ex}{\bf}}



\def\_{\rule{.3em}{.15ex}}  % Get underscore by typing \_.


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%  Line Spacing (e.g., \ls{1} for single, \ls{2} for double, even \ls{1.5})
%%

\newcommand{\ls}[1]
   {\dimen0=\fontdimen6\the\font
    \lineskip=#1\dimen0
    \advance\lineskip.5\fontdimen5\the\font
    \advance\lineskip-\dimen0
    \lineskiplimit=.9\lineskip
    \baselineskip=\lineskip
    \advance\baselineskip\dimen0
    \normallineskip\lineskip
    \normallineskiplimit\lineskiplimit
    \normalbaselineskip\baselineskip
    \ignorespaces
   }
